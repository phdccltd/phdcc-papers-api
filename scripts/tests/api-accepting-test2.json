{
  "name": "Accepting: make proposal open and paper closed",
  "pubname": "TestPubName",
  "api": [
    {
      "name": "addEditAccepting: edit proposal open",
      "method": "post",
      "uri": "/acceptings/1",
      "data": {
        "chosenstage": 1, // Proposal
        "chosenopen": true,
        //"chosenstatus": 4, // Proposal accepted
        "acceptingid": 1
      },
      "return": {
        "ret": 0
      }
    },
    {
      "name": "addEditAccepting: edit paper closed",
      "method": "post",
      "uri": "/acceptings/1",
      "data": {
        "chosenstage": 2, // Paper
        "chosenopen": false,
        "chosenstatus": 4, // Proposal accepted
        "acceptingid": 2
      },
      "return": {
        "ret": 0
      }
    }
  ]
}
